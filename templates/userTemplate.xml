<templateSet group="userTemplate">
  <template name="comt" value="/**&#10; * @package: $package$&#10; * @description: $description$&#10; * @author: $user$&#10; * @date: $date$&#10; **/" description="standred comment" toReformat="false" toShortenFQNames="true">
    <variable name="package" expression="currentPackage()" defaultValue="" alwaysStopAt="false" />
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="user" expression="lowercaseAndDash(&quot;zhangxiancheng&quot;)" defaultValue="zxc" alwaysStopAt="false" />
    <variable name="date" expression="date(&quot;yyyy-MM-dd hh:mm&quot;)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pi" value="private Integer $END$" description="private Integer" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ps" value="private String $END$" description="private String" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pd" value="private Double $END$" description="private Double" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pb" value="private Boolean $END$" description="private Boolean" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="anoy" value="$var$ -&gt; $END$" description="lambda" toReformat="false" toShortenFQNames="true">
    <variable name="var" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pl" value="private Long $END$" description="private Long" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="fbi" value="//FBI warning : magic,don't touch!" description="warning commentj" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="add" value="// add by zxc on $date$ : $desc$" description="注释" toReformat="false" toShortenFQNames="true">
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="desc" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="today" value="$date$" description="获取今天的日期" toReformat="false" toShortenFQNames="true">
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="logger" value="private Logger log = LoggerFactory.getLogger($class$.class);" description="增加日志" toReformat="false" toShortenFQNames="true">
    <variable name="class" expression="qualifiedClassName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>